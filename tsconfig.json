{
  "files": [],
  "references": [
    {
      "path": "./tsconfig.node.json"
    },
    {
      "path": "./tsconfig.app.json"
    }
  ],
  "compilerOptions": {
    "types": ["node"],
    "rootDir": ".",
    "baseUrl": ".",
    "sourceMap": true,
    "paths": {
      "@/*": ["./src/*"],
      "@/": ["./src/"]
    },
    "alwaysStrict": true,
    "noImplicitAny": false
  },
  "include": [
    "./test/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}
